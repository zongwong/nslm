            <text>未打卡</text>
          </view>
        </view> 
      </view> -->
      <view @tap="lookMoreRecord" class="lookMoreRecord" wx:if="{{start>0}}">查看更多记录</view>
      <view class="item {{item.status==2?'success':''}} {{item.status==0?'error':''}}" wx:for="{{showList}}" wx:key="record_{{index}}">
        <view class="item_title">
          <view class="item_title_left">
            <text class="status_icon"></text>
            <text>{{item.date}}</text>
            <text wx:if="{{item.status==2}}" class="greenColor">获得奖金：{{item.data.reward}}元</text>
            <text wx:if="{{item.status==0}}" class="greenColor">扣除奖金：{{activity.price}}元</text>
            <text wx:if="{{item.status==1 || item.status == 3}}" class="greyColor">任务进度：{{index + 1}}/{{activity.days}}</text>
          </view>
          <view class="item_title_right">
            <text wx:if="{{item.status==0}}" style="background-color: #e90000;">未打卡</text>
             <text wx:if="{{item.status==1}}">未打卡</text>
            <text wx:if="{{item.status==2}}" style="background-color: #52b700;">已打卡</text>
            <text wx:if="{{item.status==3}}" class="today" @tap="toSign">打卡</text>
