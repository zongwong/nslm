<script>
import wepy from "wepy";
import interfaces from "../../interface/index";
import http from "../../utils/request"
import _ from "../../utils/tools"
import api from "../../config"
import explain from "../../components/public/explain"
export default class Record extends wepy.page {
  config = {
    navigationBarTitleText: "打卡记录"
  };
  components = {
    explain:explain
  };

  data = {
    status: false,
    exTitle: "女神勋章",
    exDesc: "用于兑换课程和礼物",
    exContent: "1、女神勋章是女神们自律积极、年轻活力、变美蜕变、尊贵身份的象征；2、女神勋章可通过参加活动，并完成打卡任务获得；3、女神勋章可以用来兑换女神蜕变课程和礼物；",
    imgs: null,
    info: [],
    id: '',
    activity: null,
    recordList: [],
    showList: [],
    recorded: {},
    dayed: 0,
    width: '40rpx',
    per: '0',
    start: 0,
    end: 30,
  };

  computed = {
    now() {
      return +new Date();
    }
  };

  methods = {
    exModal() {
      this.status = true;
    },
    toSign(e) {
      let nowTime = new Date()
      let nowMin = parseInt(nowTime.getHours()) * 3600 + parseInt(nowTime.getMinutes())

      let startArr = this.activity.sign_start_time.split(':')
      let endArr = this.activity.sign_end_time.split(':')

      let startMin = parseInt(startArr[0]) * 3600 + parseInt(startArr[1])
      let endMin = parseInt(endArr[0]) * 3600 + parseInt(endArr[1])
      console.log(nowMin,startMin,endMin)
      if(nowMin<startMin || nowMin>endMin){
        wepy.showModal({
          title: '提示',
          content: '不在打卡时段内'
        })
        return false
      }

      let dayed = this.dayed + 1
      wepy.navigateTo({
        url: `/pages/course/reply?type=sign&id=${this.id}&dayed=${dayed}&days=${this.activity.days}&title=${this.activity.title}`
      })
    },
    lookMoreRecord() {
      if(this.start>0){
        let start = this.start-10
        start = start<0?0:start
        let list = this.recordList.slice(start,this.start)
        this.showList = list.concat(this.showList)
        this.start = start
        this.$apply()
      }
    }
  };
  async onLoad(options){
    this.imgs = this.$parent.globalData.imgs
    this.userInfo = this.$parent.globalData.userInfo
    this.id = options.id
    this.$apply()
  }
  async onShow() {
    // 数据获取
    const data = await interfaces.getActivityInfo(this.id)
    if (data.code) {
      this.activity = data.info
      this.$apply()
    }
    await this.getProgress(this.id)
    
    // 记录构建
    let recordList = []
    // 0 已改期 1未打卡 2已打卡 3打卡
    for (let index = 0; index < this.activity.days; index++) {
      let status = 2 // 默认找到数据就是已打卡
      let date = _.getNextTime(_.getDate(this.activity.start_time),index)
      let data = this.recorded[date]
      if (typeof data === 'undefined'){
        let rs = _.timeDir(date)
        if(rs === 1 ){ // 小于今天 过期
          status = 0
        } else if(rs === 2 ){ 
          // 未来的 -> 未打卡
          status = 1
        }else{
          // 今天的还未打卡 ->打卡
          status = 3
        }
      }
      recordList.push({
        date: date,
        status: status,
        data: data
      })
    }
    this.recordList = recordList

    let index = _.getDateDiff(_.getDate(this.activity.start_time),_.getToday())
    let start = 0,end=30;
    if(index<=10){

    }else{
      start = index-11
      end = index + 20
    }

    this.showList = recordList.slice(start,end)
    this.start = start
    this.end = end

    // 进度条
    this.width = Math.floor(this.dayed / data.info.days * 420) + 40
    this.per = Math.floor(this.dayed / data.info.days * 100)
    this.$apply()


  }
  async getProgress(id) {
    const { data } = await http({
      url: api.activity.myprogress.url,
      method: api.activity.myprogress.method,
      data: {
        aid:id
      }
    },true)
    if ( data.code ){
      this.info = data.info
      this.dayed = data.info.length
      if(data.info.length){
        let recorded = {}
        data.info.forEach((item)=>{
          let date = _.getDate(item.c_time)
          recorded[date] = item
        })
        this.recorded = recorded
      }
      this.$apply()
    }
  }
  onReachBottom() {
    if(this.end<=this.recordList.length){
      let list = this.recordList.slice(this.end,this.end+10)
      this.showList = this.showList.concat(list)
      this.end+=10
      this.$apply()
    }
  }
  onShareAppMessage() {
    return interfaces.activityShare(this.activity.title, this.activity.id, this.activity.img)
  }
}
</script>
