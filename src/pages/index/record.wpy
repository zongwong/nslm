      </view> -->
      <view class="userdata">
        <view class="days">{{dayed}}</view>
        <view class="txt">累计打卡</view>
        <view class="alldata">总计天数:{{activity.days}}天   |   打卡完成将获得 女神勋章*1 <text class="ns_icon" @tap="exModal"></text> </view>
        <view class="progress">
          <view class="progressinner" style="width:{{width}}rpx"><text class="perNum">{{per}}%</text></view>
        </view>
      </view>
    </view>
    <view class="info_block active_block">
      <view class="title singleTextOverflow">{{activity.title}}</view>
      <view class="desc">{{activity.desrc}}</view>
    </view>
    <view class="list member_block">
      <view class="title">
        <navigator url="/pages/user/mood?type=circle&id={{activity.id}}">打卡圈</navigator>
        <navigator url="/pages/user/top?id={{activity.id}}" class="top"><text class="top_icon"></text>排行榜</navigator>
      </view>
      <view class="player">
        <image src="{{item}}" wx:for="{{activity.head_imgs}}" wx:key="player_{{index}}"></image>
      </view>
    </view>
    <view class="list">
      <view class="title">
        <view>打卡进度<text class="status">进行中</text></view>
        <view class="greyColor">总奖金：{{activity.days*activity.price}}元/{{activity.days}}天</view>
      </view>
      <!-- <view class="item success">
        <view class="item_title">
          <view class="item_title_left">
            <text class="status_icon"></text>
            <text>11月22日 10:28</text>
            <text class="greenColor">获得奖金：5元</text>
          </view>
          <view class="item_title_right">
